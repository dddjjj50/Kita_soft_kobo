#include<stdio.h>

int main(void) {

	char str[] = "hello world";
	char *p= str;

	*p ='H';
	*(p+6) = 'W';

	printf("%s", str);

	return 0;


	str[]=''



}
/*
char　str[] = "hello world"; 

添え字
str[0]='h' 
str[1]='e' 
str[2]='l' 
添え字じゃない表現、 
*p=’h’ 　//これがstr[0]
*(p+1）='e'　//str[1] 
*(p+2)='l'　//str[2]



*/
