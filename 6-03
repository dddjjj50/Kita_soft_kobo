#include<stdio.h>

int main(void) {

	int num[10];
	int i;

	for (i = 0;i < 10;i++) {
		scanf("%d", &num[i]);
	}
	printf("\n");
	printf("偶数：");
	for (i = 0;i < 10;i++) {
		if (num[i] % 2 == 0) {
			printf("%d ", num[i]);
		}
	}
	printf("\n奇数：");
	for (i = 0;i < 10;i++) {
		if (num[i] % 2 == 1) {
			printf("%d ", num[i]);
		}
	}









	return 0;
}
