#include <stdio.h>

int main(void) {

	while (1) {

		int x = 0;
		printf("数値を入力して");
		scanf("%d", &x);

		if (x >= 80) {
			printf("優\n");
		}
		else if (x >= 70 && x < 80) {
			printf("良\n");
		}
		else if (x >= 60 && x < 70) {
			printf("可\n");
		}
		else if (x < 60) {
			printf("不可\n");
		}


	}
	return 0;
}
