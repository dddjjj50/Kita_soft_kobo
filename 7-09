#include<stdio.h>

int main(void) {

	char spam[100];
	int i = 0;

	for (i = 0;i < 20;i++) {
		spam[i * 4] = 'S';
		spam[(i * 4)+1] = 'P';
		spam[(i * 4)+2] = 'A';
		spam[(i * 4)+3] = 'M';
		spam[(i * 4) + 4] = '\0';
	}

	printf("%s", spam);
	return 0;
}


/*
0 s 
1 p
2 a
3 m
4 s
5 p
6 a
7 m
8 s
9 p
10 a
11 m



*/
